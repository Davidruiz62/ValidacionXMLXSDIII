<!--Longitud fija de una clave-->
  <xs:element name="clave">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value="[a-zA-Z0-9]{10}"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

